{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true,
    "codeceptjs/codeceptjs": true
  },
  "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "import/prefer-default-export": 0
  },
  "plugins": ["jasmine", "codeceptjs"]
}
